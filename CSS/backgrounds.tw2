@keyframes move-hands {
  0% {
    background-position: 0% 118%, 100% 117%, 0% 0%;
  }
  20% {
    background-position: 0% 114%, 100% 113%, 0% 0%;
  }
  30% {
    background-position: 0% 115%, 100% 114%, 0% 0%;
  }
	39.9% {
		background: url(images/hands/open_left.png) left bottom no-repeat, url(images/hands/open_right.png) right bottom no-repeat, url(images/ship_finals/ship_final.png) no-repeat;
    background-position: 0% 112%, 100% 110%, 0% 0%;
		background-size: 70%, 70%, cover;
	}
  40% {
		background-image: url(images/hands/closed_left.png), url(images/hands/closed_right.png), url(images/ship_finals/ship_final.png);
		background-size: 70%, 70%, cover;
    background-position: 0% 112%, 100% 110%, 0% 0%;
  }
	59.9% {
		background: url(images/hands/closed_left.png) left bottom no-repeat, url(images/hands/closed_right.png) right bottom no-repeat, url(images/ship_finals/ship_final.png) no-repeat;
    background-position: 0% 112%, 100% 110%, 0% 0%;
		background-size: 70%, 70%, cover;
	}
  60% {
		background-image: url(images/hands/open_left.png), url(images/hands/open_right.png), url(images/ship_finals/ship_final.png);
		background-size: 70%, 70%, cover;
    background-position: 0% 109%, 100% 107%, 0% 0%;
  }
  70% {
    background-position: 0% 110%, 100% 108%, 0% 0%;
  }
  95% {
    background-position: 0% 104%, 100% 102%, 0% 0%;
  }
  100% {
    background-position: 0% 105%, 100% 103%, 0% 0%;
  }
}

