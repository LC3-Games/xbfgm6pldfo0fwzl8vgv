::Cursors [stylesheet]

@keyframes cursor-anim {
  0%{
    cursor: url("images/cursors/frame1.png"), auto;
  }
  10%{
    cursor: url("images/cursors/frame2.png"), auto;
  }
  20%{
    cursor: url("images/cursors/frame2.png"), auto;
  }
  30%{
    cursor: url("images/cursors/frame1.png"), auto;
  }
  40%{
    cursor: url("images/cursors/frame2.png"), auto;
  }
  50%{
    cursor: url("images/cursors/frame1.png"), auto;
  }
  60%{
    cursor: url("images/cursors/frame2.png"), auto;
  }
  70%{
    cursor: url("images/cursors/frame1.png"), auto;
  }
  80%{
    cursor: url("images/cursors/frame2.png"), auto;
  }
  90%{
    cursor: url("images/cursors/frame1.png"), auto;
  }
  100%{
    cursor: url("images/cursors/frame2.png"), auto;
  }
}

@keyframes white-star-anim {
  0%{
    cursor: url("images/cursors/simple1.png"), auto;
  }
  33%{
    cursor: url("images/cursors/simple2.png"), auto;
  }
  66%{
    cursor: url("images/cursors/simple3.png"), auto;
  }
  100%{
    cursor: url("images/cursors/simple4.png"), auto;
  }
}

@keyframes yellow-star-anim {
  0%{
    cursor: url("images/cursors/cursor_1.png"), auto;
  }
  25%{
    cursor: url("images/cursors/cursor_2.png"), auto;
  }
  50%{
    cursor: url("images/cursors/cursor_3.png"), auto;
  }
  75%{
    cursor: url("images/cursors/cursor_4.png"), auto;
  }
  100%{
    cursor: url("images/cursors/cursor_5.png"), auto;
  }
}

.slowest-binary {
  animation: cursor-anim 10s infinite;
}

.slower-binary {
  animation: cursor-anim 8s infinite;
}

.slow-binary {
  animation: cursor-anim 6s infinite;
}

.fast-binary {
  animation: cursor-anim 4s infinite;
}

.faster-binary {
  animation: cursor-anim 2s infinite;
}

.fastest-binary {
  animation: cursor-anim 1s infinite;
}

.white-star-cursor {
  animation: white-star-anim 1s infinite;
}

.yellow-star-cursor {
  /* animation: yellow-star-anim 1s infinite; */
  cursor: url("images/hover_cursor.png"), auto;
}
