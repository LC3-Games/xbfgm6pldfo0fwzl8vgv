@keyframes shape-anim-gross-1 {
  30% {transform: scale(77%, 21%)}
  100% {transform: scale(-60%, -60%)}
}

@keyframes shape-anim-gross-2 {
  30% {transform: scaleX(66%); transform: rotate(200deg);}
  100% {transform: scaleX(-54%); transform: rotate(-100deg);}
}

@keyframes shape-anim-gross-3 {
  30% {transform: scaleY(45%); transform: skewY(100deg);}
  50% {transform: scaleY(95%); transform: skew(170deg)}
  100% {transform: scaleY(-75%); transform: skewY(80deg);}
}

@keyframes shape-anim-gross-4 {
  10% {transform: rotate(120deg); transform: skew(60deg)}
  80% {transform: rotate(220deg); transform: skew(120deg)}
  100% {transform: rotate(350deg); transform: skew(140deg)}
}

@keyframes shape-anim-gross-5 {
  30% {transform: matrix(0.866,0.5,-0.6,0.866,0,0); transform: scale(55%, 90%)}
  70% {transform: matrix(0.586,0.8,-0.8,0.866,0,0); transform: scale(-80%, -70%)}
  100% {transform: matrix(0.866,0.5,-0.5,0.866,0,0); transform: scale(60%, 60%)}
}

@keyframes shape-anim-gross-6 {
  30% {transform: skew(50deg); transform: scale(90%, 84%)}
  50% {transform: skew(170deg); transform: scale(-60%, -50%)}
  70% {transform: skew(20deg); transform: scale(30%, 40%)}
  100% {transform: skew(130deg); transform: scale(90%, 90%)}
}

@keyframes flash {
  0% {opacity: 0}
  30% {opacity: 0}
  100% {opacity: 1}
}

.passage.flash {
  animation: flash 4s ease forwards;
}

@keyframes flick {
  0% {background: black}
  49% {background: black}
  50% {background: white}
  100% {background: white}
}

@keyframes flick-fast {
  0% {background: black}
  99% {background: black}
  100% {background: transparent}
}


