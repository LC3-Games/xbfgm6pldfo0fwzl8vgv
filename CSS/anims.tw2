.crt::after {
  content: " ";
  display: block;
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  background: rgba(18, 16, 16, 0.1);
  opacity: 0;
  z-index: 2;
  pointer-events: none;
  animation: flicker 0.15s infinite;
}

.crt::before {
  content: " ";
  display: block;
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  background: linear-gradient(rgba(18, 16, 16, 0) 50%, rgba(0, 0, 0, 0.25) 50%), linear-gradient(90deg, rgba(255, 0, 0, 0.06), rgba(0, 255, 0, 0.02), rgba(0, 0, 255, 0.06));
  z-index: 2;
  background-size: 100% 2px, 3px 100%;
  pointer-events: none;

}
.crt {
  animation: textShadow 1.6s infinite;
}

.fileerror::before {
  animation: shadow-inset-center 1s cubic-bezier(0.250, 0.460, 0.450, 0.940) infinite alternate both;
}

@keyframes shadow-inset-center {
  0% {
    box-shadow: inset 0 0 30px 0 rgba(255, 0, 0, 1);
  }
  100% {
    box-shadow: inset 0 0 70px 0px rgba(255, 0, 0, 1);
  }
}

.flick-fast {
	animation: flick-fast 0.05s linear 1;
	height: 100%;
	width: 100%;
	position: fixed;
}

